{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Cucumber @positive Tests and Generate Report",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "e2e:positive",
        "--"
      ],
      // "postDebugTask": "generate-html-report",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development",
        "BASE_URL": "https://parabank.parasoft.com/parabank/index.htm"
      },
      "sourceMaps": true,
      "trace": true
    }
  ]
}
